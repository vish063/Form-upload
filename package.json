{
  "name": "form-upload",
  "version": "1.0.0",
  "description": "",
  "author": "Vishnu Boddeboina",
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vish063/Form-upload.git",
    "directory": "/"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "server:prod": "tsc && node -r dotenv/config ./dist/server/server.js",
    "server": "nodemon --watch server --exec ts-node -r dotenv/config ./server/index.ts",
    "lint": "npm run lint:js && npm run lint:ts && npm run lint:css",
    "lint:js": "eslint lib",
    "lint:ts": "eslint server",
    "lint:css": "stylelint --syntax=scss lib/**/*.scss",
    "format": "prettier '**/*.{css,scss,less,js,json,html,md,mdx}'",
    "format:check": "npm run format -- --check",
    "format:write": "npm run format -- --write",
    "prettier": "prettier"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "lint-staged": {
    "client/**/*.scss": [
      "stylelint --syntax=scss",
      "git add"
    ],
    "./**/*.{css,scss,less,js,json,html,md,mdx}": [
      "prettier --check",
      "git add"
    ],
    "client/**/*.js": [
      "eslint lib",
      "jest --bail --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "busboy": "^0.3.1",
    "connect-busboy": "0.0.2",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.1.0",
    "envalid": "^6.0.1",
    "express": "^4.17.3",
    "express-fileupload": "^0.3.0",
    "express-validator": "^6.6.0",
    "express-winston": "^4.0.5",
    "form-data": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.10",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "puglint-stylish": "^1.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^3.1.47",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^11.15.20",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cross-env": "^5.2.0",
    "escodegen": "1.8.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^3.1.0",
    "jquery": "2.2.4",
    "less": "2.7.1",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "stylelint-config-prettier": "^7.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.10.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "typescript": "^3.9.7"
  }
}
